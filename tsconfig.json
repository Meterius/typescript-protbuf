{
  "compilerOptions": {
    "noEmit": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "sourceMap": true,
    "strict": true,
    "target": "es2019",
    "baseUrl": "./",
    "strictNullChecks": true,
    "useUnknownInCatchVariables": false
  }
}
